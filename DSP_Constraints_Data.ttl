			dsp:minOccur "0"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "1"^^xsd:nonNegativeInteger ; 
			dsp:property dcterms:subject ;
			dsp:nonLiteralConstraint [ 
				a dsp:NonLiteralConstraint ;
				dsp:descriptionTemplate :DescriptionTemplates_Standalone_No_DT2 ;  
				dsp:valueClass :DescriptionTemplates_Standalone_No_C2 ] ] .
	:DescriptionTemplates_Standalone_No_DT2
		a dsp:DescriptionTemplate ;
		dsp:standalone "false"^^xsd:boolean ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ; 
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ; 
		dsp:resourceClass :DescriptionTemplates_Standalone_No_C2 .
	:DescriptionTemplates_Standalone_No_I1
