# ---------------
# Mapping of DSP-RDF to DSP-XML:

# <DescriptionTemplate ID="document" minOccurs="1" maxOccurs="1" standalone="yes">
:document
	a dsp:DescriptionTemplate ;
	dsp:standalone "true"^^xsd:boolean ;
	dsp:minOccur "2"^^xsd:nonNegativeInteger ;
	dsp:maxOccur "3"^^xsd:nonNegativeInteger ;

	# <ResourceClass>http://purl.org/dc/terms/Text</ResourceClass>
	dsp:resourceClass dcterms:Text ;
	
    # <StatementTemplate minOccurs="1" maxOccurs="1" type="nonliteral">
	dsp:statementTemplate [
		a dsp:NonLiteralStatementTemplate ;
		dsp:minOccur "1"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		
		# <Property>http://purl.org/dc/terms/creator</Property>
		dsp:property dcterms:creator ;
		
		# <SubPropertyOf></SubPropertyOf>
		# dsp:subPropertyOf ...
		
		# <NonliteralConstraint descriptionTemplateRef="">
		# <NonLiteralConstraint descriptionTemplateID="person">
	    dsp:nonLiteralConstraint [
			a dsp:NonLiteralConstraint ;
			dsp:descriptionTemplate :person ;
			
			# <ValueClass></ValueClass>
			# dsp:valueClass ...
			
			# <ValueURIOccurrence>disallowed</ValueURIOccurrence>
			dsp:valueURIOccurrence "disallowed"^^dsp:occurrence ;
			
			# <ValueURI></ValueURI>
			# dsp:valueURI ""^^xsd:URI
			
			# <VocabularyEncodingSchemeOccurrence>disallowed</VocabularyEncodingSchemeOccurrence>
			dsp:vocabularyEncodingSchemeOccurrence "disallowed"^^dsp:occurrence ;
			
			# <VocabularyEncodingScheme></VocabularyEncodingScheme>
			# dsp:vocabularyEncodingScheme ...
			
			# <ValueStringConstraint minOccur="0" maxOccur="1"/>
			dsp:valueStringConstraint [
				a dsp:ValueStringConstraint ;
				dsp:minOccur "0"^^xsd:nonNegativeInteger ;
				dsp:maxOccur "1"^^xsd:nonNegativeInteger 
				
				# <LiteralOption lang="" SES=""></LiteralOption>
				# dsp:literal "value string"@en^^xsd:string ;

				# <LanguageOccurrence></LanguageOccurrence>
				# dsp:languageOccurrence ""^^dsp:occurrence ;
				
				# <Language></Language>
				# dsp:language ""^^xsd:language ;
				
				# <SyntaxEncodingSchemeOccurrence></SyntaxEncodingSchemeOccurrence>
				# dsp:syntaxEncodingSchemeOccurrence ""^^dsp:occurrence ;
				
				# <SyntaxEncodingScheme></SyntaxEncodingScheme>
				# dsp:syntaxEncodingScheme ... . 
	] ] ] .
	
# <DescriptionTemplate ID="person" standalone="no">
:person
	a dsp:DescriptionTemplate ;
	dsp:standalone "false"^^xsd:boolean ;

	# <ResourceClass>http://xmlns.com/foaf/0.1/Person</ResourceClass>
	dsp:resourceClass foaf:Person ;
	
	# <StatementTemplate minOccurs="1" maxOccurs="1" type="literal">
	dsp:statementTemplate [
		a dsp:LiteralStatementTemplate ;
		dsp:minOccur "1"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		
		# <Property>http://xmlns.com/foaf/0.1/name</Property>
		dsp:property foaf:name ;
		
		# <SubPropertyOf></SubPropertyOf>
		# dsp:subPropertyOf ...
		
		# <LiteralConstraint>
		dsp:literalConstraint [
			a dsp:LiteralConstraint ;
			
			# <LiteralOption lang="" SES=""></LiteralOption>
			# dsp:literal "literal"@en^^xsd:string ;
			dsp:literal "literal"^^xsd:string ;
			
			# <LanguageOccurrence>disallowed</LanguageOccurrence>
			dsp:languageOccurrence "disallowed"^^dsp:occurrence ;
			
			# <Language></Language>
			dsp:language ""^^xsd:language ;
			
			# <SyntaxEncodingSchemeOccurrence></SyntaxEncodingSchemeOccurrence>
			dsp:syntaxEncodingSchemeOccurrence ""^^dsp:occurrence 
			
			# <SyntaxEncodingScheme></SyntaxEncodingScheme>
			# dsp:syntaxEncodingScheme ... .
	] ] .

