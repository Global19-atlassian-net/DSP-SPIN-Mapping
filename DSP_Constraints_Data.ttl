			dsp:literalConstraint [
				a dsp:LiteralConstraint ;
				dsp:literal "literal" ;
				dsp:literal "literal"@en ;
				dsp:languageOccurrence "mandatory"^^dsp:occurrence 
				] ] .
	:LiteralValueConstraints_LiteralLanguageConstraint_I1
