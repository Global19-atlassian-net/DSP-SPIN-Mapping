# ---------------
# complete running example

	:bookDescriptionTemplate # 5.1. Identifier
		a dsp:DescriptionTemplate ;
		dsp:standalone "true"^^xsd:boolean ; # 5.2. Standalone
		dsp:minOccur "1"^^xsd:nonNegativeInteger ; # 5.3. Minimum occurrence constraint
		dsp:maxOccur "infinity"^^xsd:nonNegativeInteger ; # 5.4. Maximum occurrence constraint
		dsp:resourceClass swrc:Book ; # 5.5. Resource Class Membership Constraint
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ; # 6.1. Minimum occurrence constraint
			dsp:maxOccur "5"^^xsd:nonNegativeInteger ; # 6.2. Maximum occurrence constraint
			dsp:property dcterms:subject ; # 6.4.1. Property list constraint
			dsp:nonLiteralConstraint [ # 6.3. Type constraint
				a dsp:NonLiteralConstraint ;
				dsp:descriptionTemplate :subjectDescriptionTemplate ; # 6.6.1. Description template reference
				dsp:valueClass skos:Concept ; # 6.6.2. Class membership constraint
				dsp:valueURIOccurrence "mandatory"^^dsp:occurrence ; # 6.6.3.1. Value URI occurrence constraint
				dsp:valueURI :ComputerScience, :SocialScience, :Librarianship ; # 6.6.3.2. Value URI list constraint
				dsp:vocabularyEncodingSchemeOccurrence "mandatory"^^dsp:occurrence ; # 6.6.4.1. Vocabulary encoding scheme occurrence constraint
				dsp:vocabularyEncodingScheme :BookSubjects ; # 6.6.4.2. Vocabulary encoding scheme list constraint
				dsp:valueStringConstraint [
					a dsp:ValueStringConstraint ;
					dsp:minOccur "1"^^xsd:nonNegativeInteger ; # 6.6.5.1. Minimum occurrence constraint
					dsp:maxOccur "1"^^xsd:nonNegativeInteger ; # 6.6.5.2. Maximum occurrence constraint
					dsp:literal "Computer Science"@en , "Computer Science"^^xsd:string ; # 6.5.1. Literal list constraint
					dsp:literal "Social Science"@en , "Social Science"^^xsd:string ;
					dsp:literal "Librarianship"@en , "Librarianship"^^xsd:string 
#					dsp:languageOccurrence "optional"^^dsp:occurrence ; # 6.5.2. Literal language constraint
#					dsp:language "en"^^xsd:language ; # 6.5.3. Literal language list constraint
#					dsp:syntaxEncodingSchemeOccurrence "optional"^^dsp:occurrence ; # 6.5.4. Syntax Encoding Scheme constraint
#					dsp:syntaxEncodingScheme xsd:string # 6.5.5. Syntax Encoding Scheme list constraint
				] 
			] 
		] .
	
	:ArtficialIntelligence
		a owl:Thing, swrc:Book ;
		dcterms:subject :ComputerScience .
	:ComputerScience
		a owl:Thing, skos:Concept ;
		dcam:memberOf :BookSubjects ;
		skos:inScheme :BookSubjects ;
		rdf:value "Computer Science"@en .
	:BookSubjects
		a owl:Thing, skos:ConceptScheme .

