# ---------------
# Description Templates - Minimum occurrence constraint


	# The minimum number of times this kind of description must appear in the Description Set. 
	# -----
	:DescriptionTemplate_MinimumOccurrenceConstraint_DT1
		a dsp:DescriptionTemplate ;
		dsp:minOccur "2"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "3"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :DescriptionTemplate_MinimumOccurrenceConstraint_C1 .
	:DescriptionTemplate_MinimumOccurrenceConstraint_I1
		a :DescriptionTemplate_MinimumOccurrenceConstraint_C1 , owl:Thing .
	:DescriptionTemplate_MinimumOccurrenceConstraint_I2
		a :DescriptionTemplate_MinimumOccurrenceConstraint_C1 , owl:Thing .

	# dsp:minOccur <= dsp:maxOccur
	# -----
#	:DescriptionTemplate_MinimumOccurrenceConstraint_DT2
#		a dsp:DescriptionTemplate ;
#		dsp:minOccur "4"^^xsd:nonNegativeInteger ;
#		dsp:maxOccur "3"^^xsd:nonNegativeInteger .

	# Allowed values: non-negative integer
	# -----
	:DescriptionTemplate_MinimumOccurrenceConstraint_DT3
		a dsp:DescriptionTemplate ;
		dsp:minOccur "2"^^xsd:nonNegativeInteger ;
#		dsp:minOccur "2" ; # NOT commented --> constraint violation
		dsp:maxOccur "3"^^xsd:nonNegativeInteger .

# -----

# ---------------
# Description Templates - Maximum occurrence constraint


	# The maximum number of times this kind of description is allowed to appear in the Description Set. 
	# -----
	:DescriptionTemplate_MaximumOccurrenceConstraint_DT1
		a dsp:DescriptionTemplate ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :DescriptionTemplate_MaximumOccurrenceConstraint_C1 .
	:DescriptionTemplate_MaximumOccurrenceConstraint_I1
		a owl:Thing ;
		a :DescriptionTemplate_MaximumOccurrenceConstraint_C1 .
#	:DescriptionTemplate_MaximumOccurrenceConstraint_I2
#		a owl:Thing ;
#		a :DescriptionTemplate_MaximumOccurrenceConstraint_C1 .

	# Allowed values: non-negative integer or "infinity" 
	# -----
	:DescriptionTemplate_MaximumOccurrenceConstraint_DT2
		a dsp:DescriptionTemplate ;
#		dsp:maxOccur "3" ; # NOT commented --> constraint violation
		dsp:maxOccur "3"^^xsd:nonNegativeInteger ;
		dsp:minOccur "2"^^xsd:nonNegativeInteger .
		
	# Allowed values: non-negative integer or "infinity" 
	# -----
	:DescriptionTemplate_MaximumOccurrenceConstraint_DT3
		a dsp:DescriptionTemplate ;
		dsp:maxOccur "infinity"^^xsd:string ; 
#		dsp:maxOccur "3" ; # NOT commented --> constraint violation
		dsp:minOccur "2"^^xsd:nonNegativeInteger .

# -----

# ---------------
# Description Templates - Resource Class Membership Constraint


	:DescriptionTemplates_ResourceClassMembershipConstraint_DT1
		a dsp:DescriptionTemplate ;
		dsp:standalone "true"^^xsd:boolean ;
		dsp:minOccur "1"^^xsd:nonNegativeInteger ; 
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ; 
		dsp:resourceClass :DescriptionTemplates_ResourceClassMembershipConstraint_C1 ;
		dsp:resourceClass :DescriptionTemplates_ResourceClassMembershipConstraint_C2 ;
		dsp:resourceClass :DescriptionTemplates_ResourceClassMembershipConstraint_C3 ;
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
			dsp:minOccur "2"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "2"^^xsd:nonNegativeInteger ; 
			dsp:property dcterms:subject ] .
	:DescriptionTemplates_ResourceClassMembershipConstraint_I1
		a :DescriptionTemplates_ResourceClassMembershipConstraint_C2 , owl:Thing ;
		dcterms:subject :DescriptionTemplates_ResourceClassMembershipConstraint_I2 ;
		dcterms:subject :DescriptionTemplates_ResourceClassMembershipConstraint_I3 .

# -----

# ---------------
# Statement Templates - Minimum occurrence constraint


	# The minimum number of times this kind of statement must appear in the enclosing Description. 
	# -----
	:StatementTemplate_MinimumOccurrenceConstraint_DT1
		a dsp:DescriptionTemplate ;
		dsp:resourceClass :StatementTemplate_MinimumOccurrenceConstraint_C1 ;
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
			dsp:minOccur "2"^^xsd:nonNegativeInteger ;
			dsp:property dcterms:creator ] .
	:StatementTemplate_MinimumOccurrenceConstraint_I1
		a owl:Thing ;
		a :StatementTemplate_MinimumOccurrenceConstraint_C1 ;
		dcterms:creator :StatementTemplate_MinimumOccurrenceConstraint_I2 ;
		dcterms:creator :StatementTemplate_MinimumOccurrenceConstraint_I3 .
		
	# dsp:minOccur <= dsp:maxOccur
	# -----
	:StatementTemplate_MinimumOccurrenceConstraint_DT2
		a dsp:DescriptionTemplate ;
		dsp:resourceClass :StatementTemplate_MinimumOccurrenceConstraint_C2 ;
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "2"^^xsd:nonNegativeInteger ;
			dsp:property dcterms:creator ] .
			
	# Allowed values non-negative integer
	# -----
	:StatementTemplate_MinimumOccurrenceConstraint_DT3
		a dsp:DescriptionTemplate ;
		dsp:resourceClass :StatementTemplate_MinimumOccurrenceConstraint_C3 ;
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
#			dsp:minOccur "1" ; # NOT commented --> constraint violation
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "2"^^xsd:nonNegativeInteger ;
			dsp:property dcterms:creator ] .

# -----

# ---------------
# Statement Templates - Maximum occurrence constraint


	# The maximum number of times this kind of statement is allowed to appear in the enclosing Description.
	# -----
	:StatementTemplate_MaximumOccurrenceConstraint_DT1
		a dsp:DescriptionTemplate ;
		dsp:resourceClass :StatementTemplate_MaximumOccurrenceConstraint_C1 ;
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
			dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
			dsp:property dcterms:creator ] .
	:StatementTemplate_MaximumOccurrenceConstraint_I1
		a owl:Thing ;
		a :StatementTemplate_MaximumOccurrenceConstraint_C1 ;
#		dcterms:creator :StatementTemplate_MaximumOccurrenceConstraint_I2 ;
		dcterms:creator :StatementTemplate_MaximumOccurrenceConstraint_I3 .
		
	# Allowed values non-negative integer
	# -----
	:StatementTemplate_MaximumOccurrenceConstraint_DT2
		a dsp:DescriptionTemplate ;
		dsp:resourceClass :StatementTemplate_MaximumOccurrenceConstraint_C2 ;
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
#			dsp:maxOccur "2" ; # NOT commented --> constraint violation
			dsp:maxOccur "2"^^xsd:nonNegativeInteger ;
			dsp:property dcterms:creator ] .
			
	# Allowed values non-negative integer
	# -----
	:StatementTemplate_MaximumOccurrenceConstraint_DT3
		a dsp:DescriptionTemplate ;
		dsp:resourceClass :StatementTemplate_MaximumOccurrenceConstraint_C3 ;
		dsp:statementTemplate [
			a dsp:NonLiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
#			dsp:maxOccur "2" ; # NOT commented --> constraint violation
			dsp:maxOccur "infinity"^^xsd:string ;
			dsp:property dcterms:creator ] .

# -----

# ---------------
# Property Constraints - Property List Constraint


	:PropertyConstraints_PropertyListConstraint
		a dsp:DescriptionTemplate ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :PropertyConstraints_PropertyListConstraint_ResourceClass_1 ;
		dsp:statementTemplate [
			a dsp:LiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "5"^^xsd:nonNegativeInteger ;
			dsp:property foaf:name ;
			dsp:property foaf:familyName ;
			dsp:property foaf:firstName ] .
	:PropertyConstraints_PropertyListConstraint_I1
		a owl:Thing ;
		a :PropertyConstraints_PropertyListConstraint_ResourceClass_1 ;
#		foaf:lastName "literal"^^xsd:string ;
		foaf:name "literal"^^xsd:string ;
		foaf:familyName "literal"^^xsd:string ;
		foaf:firstName "literal"^^xsd:string .
		
	# A property list constraint cannot occur together with a sub-property constraint
	:PropertyConstraints_PropertyListConstraint_DT2
		a dsp:DescriptionTemplate ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :PropertyConstraints_PropertyListConstraint_C2 ;
		dsp:statementTemplate [
			a dsp:LiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "5"^^xsd:nonNegativeInteger ;
#			dsp:property :p1 ;
			dsp:subPropertyOf :p2 ] .

# -----

# ---------------
# Property Constraints - Sub-Property Constraint


	:PropertyConstraints_Sub-PropertyConstraint_DT1
		a dsp:DescriptionTemplate ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :PropertyConstraints_Sub-PropertyConstraint_C1 ;
		dsp:statementTemplate [
			a dsp:LiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "5"^^xsd:nonNegativeInteger ;
			dsp:subPropertyOf :superP ] .
	:subP1 rdfs:subPropertyOf :superP .
	:subP2 rdfs:subPropertyOf :superP .
	:PropertyConstraints_Sub-PropertyConstraint_I1
		a :PropertyConstraints_Sub-PropertyConstraint_C1 , owl:Thing ;
#		:otherP "" ;
		:superP "" ;
		:subP1 "" ;
		:subP2 "" .

# -----

# ---------------
# Literal Value Constraints - Literal List Constraint

	# -----
	:LiteralValueConstraints_LiteralListConstraint_DT1
		a dsp:DescriptionTemplate ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :LiteralValueConstraints_LiteralListConstraint_C1 ;
		dsp:statementTemplate [
			a dsp:LiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "20"^^xsd:nonNegativeInteger ;
			dsp:property foaf:name ;
			dsp:literalConstraint [
				a dsp:LiteralConstraint ;
				dsp:literal "literal 1"@en ;
				dsp:literal "literal 2"@de ;
				dsp:literal "3"^^xsd:string ;
				dsp:literal "4"^^xsd:nonNegativeInteger 
				] ] .
	:LiteralValueConstraints_LiteralListConstraint_I1
		a owl:Thing ;
		a :LiteralValueConstraints_LiteralListConstraint_C1 ;
#		foaf:name "XXXXX" ; # different literal # NOT commented --> constraint violation
		foaf:name "literal 1"@en ;
#		foaf:name "literal 1"@de ; # different language tag # NOT commented --> constraint violation
		foaf:name "literal 2"@de ;
		foaf:name "3"^^xsd:string ;
#		foaf:name "3"^^xsd:nonNegativeInteger ; # different datatype # NOT commented --> constraint violation
		foaf:name "4"^^xsd:nonNegativeInteger .
		
	# As a literal list constraint is given, no literal language constraint can be given
	# -----
	:LiteralValueConstraints_LiteralListConstraint_DT2
		a dsp:DescriptionTemplate ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :LiteralValueConstraints_LiteralListConstraint_C2 ;
		dsp:statementTemplate [
			a dsp:LiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "20"^^xsd:nonNegativeInteger ;
			dsp:property foaf:name ;
