# ---------------
# Literal Value Constraints - Syntax Encoding Scheme Constraint


	# mandatory
	:LiteralValueConstraints_SyntaxEncodingSchemeConstraint_1
		a dsp:DescriptionTemplate ;
		dsp:minOccur "0"^^xsd:nonNegativeInteger ;
		dsp:maxOccur "1"^^xsd:nonNegativeInteger ;
		dsp:resourceClass :LiteralValueConstraints_SyntaxEncodingSchemeConstraint_ResourceClass_1 ;
		dsp:statementTemplate [
			a dsp:LiteralStatementTemplate ;
			dsp:minOccur "1"^^xsd:nonNegativeInteger ;
			dsp:maxOccur "3"^^xsd:nonNegativeInteger ;
			dsp:property foaf:name ;
			dsp:literalConstraint [
				a dsp:LiteralConstraint ;
				dsp:literal "1"^^xsd:string ;
				dsp:literal "2"@en ;
				dsp:syntaxEncodingSchemeOccurrence "mandatory"^^dsp:occurrence ] ] .
	:LiteralValueConstraints_SyntaxEncodingSchemeConstraint_I1
		a :LiteralValueConstraints_SyntaxEncodingSchemeConstraint_ResourceClass_1 , owl:Thing ;
		foaf:name "1"^^xsd:string ;
		foaf:name "2"@en .

